{
    "name": "travy/cake-tracking",
    "description": "Request tracking middleware used for CakePHP 3 applications",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeTracking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeTracking\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
